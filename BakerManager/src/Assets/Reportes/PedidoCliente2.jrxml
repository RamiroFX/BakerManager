	<parameter name="fecha_inicio" class="java.sql.Timestamp"/>
	<parameter name="fecha_fin" class="java.sql.Timestamp"/>
	<queryString>
		<![CDATA[SELECT ROUND(SUM(pedido_detalle.CANTIDAD*(pedido_detalle.PRECIO-(pedido_detalle.PRECIO*pedido_detalle.DESCUENTO)/100))) as TOTAL, CLIENTE.ENTIDAD
FROM pedido_detalle,
	pedido_cabecera,
	cliente
WHERE 
	 pedido_cabecera.id_pedido_cabecera = pedido_detalle.id_pedido_cabecera 
	 AND cliente.id_cliente = pedido_cabecera.id_cliente 
	 AND pedido_cabecera.tiempo_recepcion BETWEEN $P{fecha_inicio}  AND $P{fecha_fin}
GROUP BY CLIENTE.ENTIDAD
ORDER BY TOTAL DESC]]>
	</queryString>
